#!/bin/bash

set -eu

source $(dirname $(realpath ${BASH_SOURCE[0]}))/tmox-header

SUBCMD_HEAD=$1
shift
EXEC_PATH="$(dirname $(realpath ${BASH_SOURCE[0]}))/tmox-$SUBCMD_HEAD"

if [[ ! -f $EXEC_PATH ]] || [[ $SUBCMD_HEAD == "header" ]]; then
  echo "$0: error: subcommand not found."
  exit
fi

bash $EXEC_PATH "$@"
